{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoadTestConfig": {
    "BaseUrl": "http://httpstat.us/",
    "TimeoutPolicyConfig": {
      "seconds": 180
    },
    "RetryPolicyConfig": {
      "RetryCount": 1
    },
    "CircuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  }
}
