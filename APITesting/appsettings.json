{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoadTestConfig": {
    "BaseUrl": "http://httpstat.us/",
    "TimeoutPolicyConfig": {
      "Seconds": 180
    },
    "RetryPolicyConfig": {
      "RetryCount": 2
    },
    "CircuitBreakerPolicyConfig": {
      "AllowExceptions": 2,
      "BreakDuration": 30
    }
  }
}
